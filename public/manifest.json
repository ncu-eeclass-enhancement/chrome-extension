{
    "manifest_version": 3,
    "name": "Cookie Reader Extension",
    "description": "This is our sample extension",
    "version": "1.0",
    "action": {
        "default_popup": "index.html",
        "default_icon": "favicon.ico"
    },
    "permissions": [
      "cookies",
      "tabs"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "*://*.ncueeclass.ncu.edu.tw/",
      "http://*/*",
      "https://*/*"
    ]
  }